{
  "root": true,
  "extends": [
    "@react-native",
    "prettier"
  ],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "no-console": "error",
    "prettier/prettier": [
      "error",
      {
        "quoteProps": "consistent",
        "singleQuote": true,
        "tabWidth": 2,
        "trailingComma": "es5",
        "useTabs": false
      }
    ]
  }
}
