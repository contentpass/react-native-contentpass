{
  "preset": "react-native",
  "setupFilesAfterEnv": ["./jest-setup.ts"],
  "transform": {
    "^.+\\.(js)$": [
      "babel-jest",
      {
        "plugins": [
          "babel-plugin-syntax-hermes-parser"
        ]
      }
    ],
    "^.+\\.(ts|tsx)$": "babel-jest"
  },
  "modulePathIgnorePatterns": [
    "./examples/*/node_modules",
    "./packages/*/lib/",
    "./packages/*/ios/",
    "./packages/*/android/"
  ]
}

