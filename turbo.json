{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "typecheck": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "prepare": {
      "outputs": [
        "lib/**"
      ]
    },
    "build:android": {
      "inputs": [
        "package.json",
        "packages/react-native-contentpass/package.json",
        "packages/react-native-contentpass/android",
        "packages/react-native-contentpass/src/*.ts",
        "packages/react-native-contentpass/src/*.tsx",
        "examples/*/package.json",
        "examples/*/android",
        "!examples/*/android/.gradle",
        "!examples/*/android/build",
        "!examples/*/android/app/build"
      ],
      "outputs": []
    },
    "build:ios": {
      "inputs": [
        "package.json",
        "packages/react-native-contentpass/package.json",
        "packages/react-native-contentpass/*.podspec",
        "packages/react-native-contentpass/ios",
        "packages/react-native-contentpass/src/*.ts",
        "packages/react-native-contentpass/src/*.tsx",
        "examples/*/package.json",
        "examples/*/ios",
        "!examples/*/ios/build",
        "!examples/*/ios/Pods"
      ],
      "outputs": []
    }
  }
}
